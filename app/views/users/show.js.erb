<% puts "@tab = #{@tab}"
  if @tab == 'friends' -%>
$('#users').append('<%= j render("users/users", :users => @users, :id => @user2.id, :tab => @tab, :last_gift_id => @last_gift_id) %>');
<% end -%>
<% if @tab == 'balance' -%>
$('#gifts').append('<%= j render("users/gifts", :gifts => @gifts, :id => @user2.id, :tab => @tab, :last_gift_id => @last_gift_id) %>');
<% end -%>