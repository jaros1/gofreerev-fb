<%# hide all rows with format gift-<gift_id>- %>
(function(){
    var re = new RegExp("^gift-<%= @gift_id %>-") ;
    var gifts = document.getElementById('gifts') ;
    var trs = gifts.rows ;
    var lng = trs.length ;
    var tr ;
    for (var i=0 ; i<lng ; i++) {
        tr = trs[i] ;
        if (tr.id && tr.id.match(re)) tr.style.display = 'None' ;
    }
})();