<%# insert accepted gift in new_messages_buffer_div buffer in page header
    post ajax processing is done by JS function insert_update_gifts (my.js)
    old not accepted gift is ajax replaced with accepted gift
-%>
<% if @api_gifts -%>
document.getElementById('new_messages_buffer_div').innerHTML = '<%= j render("new_messages_count") %>' ;
insert_update_gifts() ;
<% end -%>
<% if @errors2.size > 0 %>
<%# error response - inject error into tasks_errors2 table and move error to placeholder table within gifts/index page -%>
$('#tasks_errors2').append('<%= j render('shared/ajax_errors2') %>');
move_tasks_errors2();
<% end %>